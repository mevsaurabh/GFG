class Solution {
    public double maxVolume(double perimeter, double area) {
        double ans = Math.pow((perimeter - Math.sqrt(Math.pow(perimeter, 2) - (24 * area))) / 12, 2) *
                     (perimeter / 4 - 2 * ((perimeter - Math.sqrt(Math.pow(perimeter, 2) - (24 * area))) / 12));
        return ans;
    }
}
