class Solution {
    public int[] Series(int n) {
        int[] fibonacciSeries = new int[n + 1]; 
        int modulo = 1000000007; 
        fibonacciSeries[0] = 0; 
        fibonacciSeries[1] = 1; 

        for (int i = 2; i <= n; i++) {
            fibonacciSeries[i] = (fibonacciSeries[i - 1] + fibonacciSeries[i - 2]) % modulo;
        }

        return Arrays.copyOfRange(fibonacciSeries, 0, n + 1); 
    }
}
