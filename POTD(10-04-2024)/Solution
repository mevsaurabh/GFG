class Solution {
    static int findSingle(int n, int arr[]) {
        int result = 0;
        
        // Perform XOR operation on all elements
        for (int i = 0; i < n; i++) {
            result ^= arr[i];
        }
        
        return result;
    }
}
/*
    Time complexity : O(n)
    Space Complexity : O(1)
    */
